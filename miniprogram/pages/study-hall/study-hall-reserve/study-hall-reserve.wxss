page{
  background-color: #eee;
}
#image-wrapper{
  width: 100%;
  height: 120px;
  background: white;
  margin: 0 auto;
  /* border-radius: 10px; */
  /* border:1px solid #ccc; */
  position: relative;
  font-size: 14px;
  font-family: PingFangSC-Regular;
  font-weight: lighter;
}
#image-wrapper .img{
  width: 100px;
  height: 100px;
  background: red;
  margin: 10px 10px;
}
#image-wrapper .name{
  position: absolute;
  top: 30px;
  left: 35%;
}
#image-wrapper .add{
  position: absolute;
  top: 60px;
  left: 35%;
  font-size: 12px;
  color: #999;
}
#content-wrapper{
  width: 100%;
  height: 212px;
  background-color:#eee;
  margin-top: 10px;
}
#content-wrapper view{
  background-color: white;
  margin-bottom: 3px;
  height: 40px;
  position: relative;
}
#content-wrapper view view{
  font-size: 14px;
  font-family:PingFangSC-Regular;
  line-height: 40px;
  margin-left: 20px;
  font-weight: lighter;
}
#content-wrapper .room-name-value{
  position: absolute;
  color: #999;
  top: 0;
  left: 80%;
  text-align: center;
}
#content-wrapper .room-time-value{
  position: absolute;
  color: #999;
  top: 0;
  left: 80%;
}
#content-wrapper .room-price-value{
  position: absolute;
  color: #999;
  top: 0;
  left: 85%;
}
#content-wrapper .user-name-value{
  position: absolute;
  font-size: 14px;
  font-family:PingFangSC-Regular;
  font-weight: lighter;
  color: #999;
  top: 0;
  left: 85%;
  height: 40px;
  line-height: 40px;
}
#content-wrapper .user-call-value{
  position: absolute;
  font-size: 14px;
  font-family:PingFangSC-Regular;
  font-weight: lighter;
  color: #999;
  top: 0;
  left: 75%;
  height: 40px;
  line-height: 40px;
}
#tips-wrapper{
  width: 90%;
  position: relative;
  top: 15px;
  height: 120px;
  margin: 0 auto;
  border-radius: 10px;
  background: #ccc;
}
#tips-wrapper view:nth-child(1){
  font-size: 14px;
  position: absolute;
  top: 10px;
  left: 10px;
}
#tips-wrapper view:nth-child(2){
  font-size: 10px;
  position: absolute;
  top: 40px;
  left: 20px;
  font-weight: lighter;
}
#tips-wrapper view:nth-child(3){
  font-size: 10px;
  position: absolute;
  top: 75px;
  left: 20px;
  font-weight: lighter;
}
#tips-wrapper view:nth-child(4){
  font-size: 10px;
  position: absolute;
  top: 96px;
  left: 20px;
  font-weight: lighter;
}
#sub{
  position: relative;
  top: 50px;
}
#sub button{
  width: 100px;
  font-size: 14px;
  background: #99CC99;
  color: white;
}